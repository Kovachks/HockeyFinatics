<div id="header" style="background-color:{{team.0.teaminfo.primaryColor}}">
    <a href="/#"><img src="/images/{{team.0.mascot}}.png" alt="" id="teamLogo" style="height: 100px; width: 150px;"></a>
      <h1 class="teamName" style="color: white">{{team.0.teaminfo.teamName}}</h1>
  </div>
  <ul class="nav" id="nav" style="background-color: {{team.0.teaminfo.secondaryColor}}"> 
      <li class="nav-item">
       
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Roster</a>
      </li>      
      <li class="nav-item">
        <a class="nav-link" id="bluej" href="/{{team.0.mascot}}/schedule">Schedule</a>
      </li>

  </ul>
<h2 class="sectionHeader">Team Roster</h2>
<table class="table table-inverse" id="playerStats">
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Nationality</th>
      <th>Team</th>
      <th>Position</th>
      <th>Games Played</th>
      <th>Goals</th>
      <th>Assists</th>
      <th>Points</th>
    </tr>
  </thead>
  <tbody>
    {{#each team}}
    <tr>
      <td>{{name}}</td>
      <td>{{age}}</td>
      <td>{{nationality}}</td>
      <td>{{team}}</td>
      <td>{{position}}</td>
      <td>{{games_played}}</td>
      <td>{{goals}}</td>
      <td>{{assists}}</td>
      <td>{{points}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>