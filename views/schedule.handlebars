  <div id="header" style="background-color:{{team.0.teaminfo.primaryColor}}">
    <img src="/images/{{team.0.mascot}}.png" alt="" id="teamLogo" style="height: 100px; width: 150px;">
      <h1 class="teamName" style="color: white">{{team.0.teaminfo.teamName}}</h1>
  </div>
  <ul class="nav" id="nav" style="background-color: {{team.0.teaminfo.secondaryColor}}"> 
      <li class="nav-item">
        <a class="nav-link active" href="/">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/{{team.0.mascot}}/schedule">Schedule</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Roster</a>
      </li>
  </ul>
<h2 class="sectionHeader">Schedule</h2>
<table class="table table-inverse" id="teamSchedule">
  <thead>
    <tr>
      <th>Date</th>
      <th>Opponent</th>
      <th>Time</th>
      <th>Timezone</th>
      <th>% To Win</th>
    </tr>
  </thead>
  <tbody>
    {{#each team}}
    <tr>
      <td>{{gameDate}}</td>
      <td>{{vs}}<img src="/images/{{opponent}}.png" alt="" class="scheduleImage"></td>
      <td>{{time}}</td>
      <td>{{timezone}}</td>
      <td>{{chanceToWin}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>